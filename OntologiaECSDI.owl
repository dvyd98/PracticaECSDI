    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#Filtres</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Representa el tipus de filtre de la cerca. Pots cercar per marca, preu o nom.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#GestorCercaAgent</IRI>
